# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Solana
# For devnet testing, use: https://api.devnet.solana.com
# For mainnet, use Helius or other RPC provider
NEXT_PUBLIC_SOLANA_RPC_URL=https://api.devnet.solana.com
NEXT_PUBLIC_SOLANA_NETWORK=devnet

# Escrow wallet for bets (must have private key access for payouts)
ESCROW_WALLET_ADDRESS=your_escrow_wallet_public_key

# Admin wallet addresses (comma-separated)
ADMIN_WALLET_ADDRESSES=wallet1,wallet2
